num=int(input('Ingrese un número: '))
i,suma=1,0
while i<num:
    if num%i==0:
        suma+=1
    i+=1
print(num,'Es impar',end='') if num%2==0 else print(num,'Es par',end='')
print(', es un número primo',end=' ') if suma==1 else print(', no es un número primo',end=' ')
print('y es mayor que 50') if num>50 else print('y es menor que 50')